/*******************ListWidget**********************/

QListWidget
{
    font-size: 14px;
    font-weight: bold;
	border: 1px solid #32435E;  
	background:rgb(60,60,108);
	color:white;
}


QListWidget::item:selected {  
    border: 0px solid #33CCFF;
    border:none;  
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #6A848C,  stop: 1.0 #0F9EAF);  
    padding:0px;  
    margin:0px;  
}  

/*******************TableWidget**********************/
QHeaderView{
    font-size:12px;
}
 
QHeaderView::section {
    color: white;
    padding: 4px;
    height:24px;
    border: 0px solid #567dbc;
    border-left:0px;
    border-right:0px;
    background: #2c3e50;/*default*/
}

QTableWidget{
    color: black;
    gridline-color: gray;
	background: rgb(245,245,245);
	selection-color: white;
	selection-background-color: qlineargradient(x1:0, y1:0, x2:0.5, y2:0.5,stop:0 rgb(0,190,255));
	border: 2px groove gray;
	border-radius: 1px;
}

/*******************LCDNumber**********************/

QLCDNumber{  
padding: 5px;  
color:#0000FF;
background:rgb(231,236,166);
border-style: solid;  
border: 2px solid gray;  
border-radius: 45px;  
width:150px;  
height:50px;  
position:center;
} 



/*******************Calendar**********************/

QCalendarWidget QWidget#qt_calendar_navigationbar { 
    background-color: rgb(52,105,175); 
}

QCalendarWidget QToolButton {
	height: 30px;
	width: 80px;
	color: white;
	font-size: 16px;
	icon-size: 32px, 32px;
	background-color: rgb(52,105,175); 
}

QCalendarWidget QMenu {
	width: 80px;
	left: 20px;
	color: white;
	font-size: 14px;
	background-color: rgb(234, 74, 100);
}

QCalendarWidget QSpinBox { 
	width: 90px; 
	font-size:22px; 
	color: white; 
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 #cccccc, stop: 1 #333333); 
	selection-background-color: rgb(136, 136, 136);
	selection-color: rgb(255, 255, 255);
}

QCalendarWidget QSpinBox::up-button { 
    subcontrol-origin: border;
    subcontrol-position: top right;
    width:32px;
}

QCalendarWidget QSpinBox::down-button {
    subcontrol-origin: border; 
    subcontrol-position: bottom right;  
    width:32px;
}
QCalendarWidget QSpinBox::up-arrow { 
    width:56px;  
    height:56px; 
}

QCalendarWidget QSpinBox::down-arrow {
    width:56px;  
    height:56px; 
 }
 
/* header row */
QCalendarWidget QWidget { 
    alternate-background-color: rgb(255, 255, 245);
}
 
/* normal days */
QCalendarWidget QAbstractItemView:enabled 
{
	font-size:22px;  
	color: rgb(180, 180, 180);  
	background-color: rgb(57,65,101);
	selection-background-color:rgb(64, 64, 64); 
	selection-color: rgb(0, 255, 0); 
}
 
/* days in other months */
QCalendarWidget QAbstractItemView:disabled { 
    color: rgb(64, 64, 64);
}



