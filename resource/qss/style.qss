/*******************GroupBox**********************/
QGroupBox {
    border-width:1px;
    border-style:solid;
    border-color:rgb(250,250,250);
	color:white;
	border-radius: 1px;
}

/*设置界面各设置框底色样式*/
QGroupBox#groupBox_PrintModel,#groupBox_Encode,#groupBox_Lis,#groupBox_Language,#groupBox_Channel,#groupBox_Net
,#groupBox_Aging,#groupBox_RawData,#groupBox_APOFFSET1,#groupBox_APOFFSET2,#groupBox_APOFFSET3,#groupBox_OptLog
,#groupBox_WriteMode,#groupBox_Separate,#groupBox_Coe
 {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #E0E0E0, stop: 1 #FFFFFF);
    border: 1px solid gray;
    border-radius: 5px;
    margin-top: 1ex; /* leave space at the top for the title */
}

QGroupBox::title {
    color:#0000FF;
	subcontrol-position: top center;
	padding:-10px;
}

/*设置界面标题框底色*/
QGroupBox#groupBox_Pinrt,#groupBox_Refv,#groupBox_Date,#groupBox_SysSet,#groupBox_SysInfo,#groupBox_Debug{
    
	background:rgb(60,60,108);
}

/*通道标题样式*/
QGroupBox#groupBox_c1::title,#groupBox_c2::title,#groupBox_c3::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
	color:#0000FF;
    padding:-20 20px;
}

/*主界面标题栏底色*/
QGroupBox#groupBox_State{
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,  stop: 1.0 rgb(70,70,78));  
}


QGroupBox#groupBox_c1,#groupBox_c3{
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,  stop: 1.0 rgb(245,245,245));  
	/*border-image:url(:/);*/
	/*border: 1px solid #0000FF;*/
    border: 1px solid gray;
    color:#0000FF;
	font-size:14px;  
}

QGroupBox#groupBox_c2{
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,  stop: 1.0 rgb(245,245,245));  
	border: 1px solid gray;
    color:#0000FF;
	font-size:14px;  
}

QGroupBox#groupBox_c2::indicator:checked{
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,  stop: 1.0 rgb(255,0,0));  
}

